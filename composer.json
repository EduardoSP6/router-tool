{
    "name": "eduardosp6/router-tool",
    "description": "Routing solution package for Laravel projects",
    "type": "library",
    "keywords": [
        "Laravel",
        "Routing",
        "Geocoding"
    ],
    "homepage": "https://github.com/EduardoSP6/router-tool",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Eduardosp6\\RouterTool\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eduardosp6\\RouterTool\\RouterToolServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Eduardo P. Sales",
            "email": "eduardo.sp6@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "illuminate/support": ">=7.30.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "minimum-stability": "dev"
}
